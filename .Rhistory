sigmas <- function(tope, r, tf=1) {
cr <- c(.12, .12, rep(0, 6))
text(1:8 - 0.48 - cr, tope, labels=expression(sigma ^ 2 == ''), pos=4,
cex=tf, family='serif')
text(1:8 + 0.02 - cr, tope - r, labels=nf, pos=4, cex=tf, family='serif')
}
